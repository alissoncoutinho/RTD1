@model IEnumerable<Barragem.Models.Rancking>

    <h2>Teste @ViewBag.pontosDefendidos</h2>

    <table class="table table-striped table-condensed table-bordered">
        <tr>
            <th>
                id
            </th>
            <th>
                data
            </th>
            <th>
                rodada
            </th>
            <th>
                rodadaId
            </th>
            <th>
                ponto
            </th>
            
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.rodada.dataFim)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.rodada.codigoSeq)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.rodada_id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.pontuacao)
                </td>
                
            </tr>
        }

    </table>

